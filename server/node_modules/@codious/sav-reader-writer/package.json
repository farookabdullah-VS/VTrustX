{
  "name": "@codious/sav-reader-writer",
  "version": "1.1.4",
  "license": "MIT",
  "author": "Bart Lenaerts <bart@codious.io>",
  "description": "Reads or write a SPSS (SAV) file to or from variable and data records",
  "keywords": [
    "sav",
    "spss",
    "reader",
    "writer"
  ],
  "engines": {
    "yarn": "please-use-npm"
  },
  "husky": {
    "hooks": {
      "pre-commit": "(npm run lint) && (npm test)"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "test": "jest --passWithNoTests",
    "version": "echo $npm_package_version"
  },
  "dependencies": {
    "moment": "2.28.0"
  },
  "devDependencies": {
    "eslint": "7.9.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "24.0.1",
    "eslint-plugin-sonarjs": "0.5.0",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "jest-extended": "0.11.5",
    "jest-junit": "11.1.0",
    "jest-sonar-reporter": "2.0.0"
  }
}